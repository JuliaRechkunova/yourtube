{
	"name": "YourTube",
	"version": "0.0.0",
	"description": "Browser extension to remove banners from Youtube",
	"icons": {
		"19": "icon19.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://www.youtube.com/*"
			],
			"js": [
				"contentscript.js"
			]
		}
	],
	"manifest_version": 2
}
